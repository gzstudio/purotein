<!DOCTYPE html>
<html>
  <head>
    <title>Purotein</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

  <header>
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
    <div class="container">
      <%= link_to "PUROTEIN", '#', id: "logo", class: "navbar-brand" %>
      <nav>
        <ul class="nav navbar-nav navbar-right">
          <li><%= link_to "all snacks",   '#' , class: "nav-link" %></li>
          <li><%= link_to "+ add snack",   '#' , class: "nav-link" %></li>
          <li><%= link_to "log in", '#' , class: "nav-link" %></li>
          <li><%= link_to "sign up", '#' , class: "nav-link" %></li>
        </ul>
      </nav>
    </div>
    </nav>
  </header>

    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>
